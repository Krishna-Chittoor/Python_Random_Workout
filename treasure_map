row3 = ["⬜️️","⬜️️","⬜️️"]
map = [row1, row2, row3]
print(f"{row1}\n{row2}\n{row3}")
position = input("Where do you want to put the treasure? ")
# 🚨 Don't change the code above 👆

#Write your code below this row 👇

position_arr = [int(n) for n in str(position) ]

#print(position_arr)

column_temp = position_arr [0]
row_temp = position_arr [1]

column =  column_temp-1
row = row_temp-1 

# print(column) 
# print(row) 

map[row][column] = 'X'

